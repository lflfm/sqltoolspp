        MENUITEM "+\tOne or more",              ID_REGEXP_FIND_ONE_OR_MORE
        MENUITEM SEPARATOR
        MENUITEM "^\tBeginning of line",        ID_REGEXP_FIND_BOL
        MENUITEM "$\tEnd of line",              ID_REGEXP_FIND_EOL
