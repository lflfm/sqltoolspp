        MENUITEM "[^]\tAny one character not in the set", 
                                                ID_REGEXP_FIND_NOT_SET
        MENUITEM "|\tOr",                       ID_REGEXP_FIND_OR
        MENUITEM "\\\tEscape Special Character", ID_REGEXP_FIND_ESC
        MENUITEM SEPARATOR
        MENUITEM "([a-zA-Z0-9])\tAlpha-numeric characters", 
                                                ID_REGEXP_FIND_ALNUM
        MENUITEM "([a-zA-Z])\tAlphabetic characters", ID_REGEXP_FIND_ALPHA
        MENUITEM "([0-9])\tDecimal digits",     ID_REGEXP_FIND_DIGIT
        MENUITEM "([0-9a-fA-F]+)\tHexidecimal digits", ID_REGEXP_FIND_XDIGIT
        MENUITEM "([a-zA-Z_$][a-zA-Z0-9_$]*)\tC-Identifier", 
                                                ID_REGEXP_FIND_IDENTIFIER
        MENUITEM "(([0-9]+\\.[0-9]*)|([0-9]*\\.[0-9]+)|([0-9]+))\tRational number", 
                                                ID_REGEXP_FIND_RATIONAL
